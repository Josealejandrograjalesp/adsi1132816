doctype
html(lang="en")
  head
    meta(charset="UTF-8")
    title Tipos de entradas (Formulario)
  body
    h1 Atributos (Formulario)
    hr
    fieldset
      legend Validar / Autocompletar
      form(method='post')
        label(for='Nombre') Nombre:
        input(type='text', id='nombre', name='nombre' required)
        br
        input(type='submit', value='Enviar')
    br
    br
    fieldset
      legend No Validar /No Autocompletar
      form(method='post', autocomplete='off', novalidate)
        label(for='Nombre') Nombre:
        input(type='text', id='nombre', name='nombre' required)
        br
        input(type='submit', value='Enviar')  